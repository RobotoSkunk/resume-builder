{
  "name": "resume-builder",
  "version": "0.1.0",
  "private": true,
  "main": "src/backend/dist/index.js",
  "author": "Edgar Lima (RobotoSkunk)",
  "description": "Una aplicación simple para construír currículums automáticamente porque me da flojera usar Word.",
  "scripts": {
    "dev": "npm run build:backend && concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev src/frontend --turbopack\" \"electron .\"",
    "dev:watch": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev src/frontend --turbopack\" \"nodemon --exec electron .\"",
    "build:backend": "tsc -p src/backend",
    "tscw": "tsc -w -p src/backend",
    "prebuild": "next telemetry disable",
    "lint": "next lint src/frontend",
    "make": "npm run build:backend && next build src/frontend && electron-builder",
    "migration:create": "ts-node ./bin/migration-create.ts",
    "electron:rebuild": "electron-rebuild better-sqlite3"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@napi-rs/canvas": "^0.1.70",
    "better-sqlite3": "^11.9.1",
    "electron-squirrel-startup": "^1.0.1",
    "framer-motion": "^12.9.7",
    "get-port-please": "^3.1.2",
    "kysely": "^0.28.2",
    "next": "15.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@eslint/eslintrc": "^3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.15.12",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "concurrently": "^9.1.2",
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "electron-forge-maker-appimage": "^26.0.12",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "packageManager": "npm@11.3.0"
}
